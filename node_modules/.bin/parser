#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/@babel/parser/bin/babel-parser.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/@babel/parser/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/@babel/parser/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/@babel/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else 
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
