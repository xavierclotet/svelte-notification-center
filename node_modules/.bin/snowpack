#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/snowpack@2.6.3/node_modules/snowpack/dist-node/index.bin.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/snowpack@2.6.3/node_modules/snowpack/dist-node/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/snowpack@2.6.3/node_modules/snowpack/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/snowpack@2.6.3/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/snowpack/dist-node/index.bin.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/snowpack/dist-node/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/snowpack/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../snowpack/dist-node/index.bin.js" "$@"
else 
  exec node  "$basedir/../snowpack/dist-node/index.bin.js" "$@"
fi
