#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/import-local/fixtures/cli.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/import-local/fixtures/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/import-local/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else 
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
