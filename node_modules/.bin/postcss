#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/postcss-cli@7.1.1/node_modules/postcss-cli/bin/postcss/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/postcss-cli@7.1.1/node_modules/postcss-cli/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/postcss-cli@7.1.1/node_modules/postcss-cli/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/postcss-cli@7.1.1/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/postcss-cli/bin/postcss/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/postcss-cli/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/postcss-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../postcss-cli/bin/postcss" "$@"
else 
  exec node  "$basedir/../postcss-cli/bin/postcss" "$@"
fi
