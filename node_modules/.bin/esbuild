#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/esbuild/bin/esbuild/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/esbuild/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/esbuild/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
