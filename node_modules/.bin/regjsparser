#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/regjsparser/bin/parser/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/regjsparser/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/regjsparser/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else 
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
