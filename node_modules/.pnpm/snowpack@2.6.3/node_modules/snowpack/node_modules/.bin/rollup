#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rollup@2.18.2/node_modules/rollup/dist/bin/rollup/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rollup@2.18.2/node_modules/rollup/dist/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rollup@2.18.2/node_modules/rollup/dist/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rollup@2.18.2/node_modules/rollup/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rollup@2.18.2/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.18.2/node_modules/rollup/dist/bin/rollup" "$@"
else 
  exec node  "$basedir/../../../../../rollup@2.18.2/node_modules/rollup/dist/bin/rollup" "$@"
fi
