#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jest-runtime@25.5.4/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jest-runtime@25.5.4/node_modules/jest-runtime/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jest-runtime@25.5.4/node_modules/jest-runtime/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jest-runtime@25.5.4/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/jest-runtime/bin/jest-runtime.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/jest-runtime/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/jest-runtime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-runtime/bin/jest-runtime.js" "$@"
else 
  exec node  "$basedir/../jest-runtime/bin/jest-runtime.js" "$@"
fi
