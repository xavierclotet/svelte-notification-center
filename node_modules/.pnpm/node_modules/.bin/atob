#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/atob.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/atob@2.1.2/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/atob/bin/atob.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/atob/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/atob/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../atob/bin/atob.js" "$@"
else 
  exec node  "$basedir/../atob/bin/atob.js" "$@"
fi
