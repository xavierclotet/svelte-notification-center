#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/json5@2.1.3/node_modules/json5/lib/cli.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/json5@2.1.3/node_modules/json5/lib/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/json5@2.1.3/node_modules/json5/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/json5@2.1.3/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/json5/lib/cli.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/json5/lib/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/json5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json5/lib/cli.js" "$@"
else 
  exec node  "$basedir/../json5/lib/cli.js" "$@"
fi
