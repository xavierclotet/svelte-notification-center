#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/uuid@3.4.0/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/uuid/bin/uuid/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/uuid/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/uuid/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../uuid/bin/uuid" "$@"
fi
