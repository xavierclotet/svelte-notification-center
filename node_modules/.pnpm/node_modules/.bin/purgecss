#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/purgecss@2.3.0/node_modules/purgecss/bin/purgecss/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/purgecss@2.3.0/node_modules/purgecss/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/purgecss@2.3.0/node_modules/purgecss/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/purgecss@2.3.0/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/purgecss/bin/purgecss/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/purgecss/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/purgecss/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../purgecss/bin/purgecss" "$@"
else 
  exec node  "$basedir/../purgecss/bin/purgecss" "$@"
fi
