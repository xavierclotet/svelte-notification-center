#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/semver@5.7.1/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/semver/bin/semver/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/semver/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/semver/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else 
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
