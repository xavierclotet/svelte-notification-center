#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/detective@5.2.0/node_modules/detective/bin/detective.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/detective@5.2.0/node_modules/detective/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/detective@5.2.0/node_modules/detective/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/detective@5.2.0/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/detective/bin/detective.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/detective/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules/detective/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detective/bin/detective.js" "$@"
else 
  exec node  "$basedir/../detective/bin/detective.js" "$@"
fi
