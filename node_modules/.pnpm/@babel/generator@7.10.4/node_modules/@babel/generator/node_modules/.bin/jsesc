#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
else 
  exec node  "$basedir/../../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
fi
