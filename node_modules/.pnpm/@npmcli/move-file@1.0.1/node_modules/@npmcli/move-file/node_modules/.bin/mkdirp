#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
