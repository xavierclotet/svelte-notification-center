#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/bin.js/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules/.pnpm/node_modules:/home/xavi/Projects/svelte-notification-center/node_modules:/home/xavi/Projects/node_modules:/home/xavi/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
